    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^6.4.22",
    "@storybook/theming": "^6.4.22",
    "@svgr/rollup": "^6.2.1",
    "@svgr/webpack": "^6.2.1",
    "@testing-library/dom": "^8.13.0",
